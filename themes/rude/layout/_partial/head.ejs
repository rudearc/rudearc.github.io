    <![endif]-->

    <%
        if(!config.author) {
            config.author = ""
        }

        var title = title = config.title;
        if (is_archive()){
            title = 'Arsip | ' + config.author;
        } else if (is_post()){
            title = page.title;
        } else if (is_page()){
            title = page.title + ' | ' + config.author;
        } else if (is_tag()){
            title = 'Tulisan tentang' + page.tag;
        }
    
    %>

    <title><%= title %></title>

</head>